*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}




.btn{
	width:10%;
	height:25px;
	border-radius: 5px;
	font-size: 17px;
    margin-left: 5%;
    margin-top: 20px;
    background-color: rgb(49, 99, 236);
    outline: none;
    border: 0;
    cursor: pointer;
    color: #fff;
    transition: all .1s ease-in-out;
}

.btn:hover{
	background-color: rgb(21, 67, 194);
}

#rejected{
	margin-left:25%
}


#table-data{
	margin-top: 20px;
	margin-left:10%;
	width:80%;
	height:100%;
	border-collapse:collapse;
	
}

